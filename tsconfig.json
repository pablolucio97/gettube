{
  "compilerOptions": {
    "target": "es2016",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "module": "esnext",
    "jsx": "preserve"
  },
  "files": [
    "src/@types/styled.d.ts",
    "src/themes/styled.d.ts"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "styled.d.ts",
    "src/themes/styled.d.ts"
, "commitlintrc.js"  ],
  "exclude": [
    "node_modules"
  ]
}
